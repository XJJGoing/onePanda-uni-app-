<scroll-view class="person"><view class="content"><view class="header"><image src="{{avatarUrl}}"></image><text>{{nickName}}</text></view><view class="main"><view class="main-setting"><van-cell-group vue-id="1" title="设置" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><van-cell vue-id="{{('2')+','+('1')}}" title="反馈" is-link link-type="navigateTo" url="../feedback/feedback" data-com-type="wx" bind:__l="__l"></van-cell><van-cell vue-id="{{('3')+','+('1')}}" title="关于我们" is-link link-type="navigateTo" url="../version/version" data-com-type="wx" bind:__l="__l"></van-cell><block wx:if="{{user.isRoot==1}}"><block><van-cell vue-id="{{('4')+','+('1')}}" title="审核文章" is-link link-type="navigateTo" url="../verifyArticle/verifyArticle" data-com-type="wx" bind:__l="__l"></van-cell></block></block></van-cell-group></view><view class="main-userInfo"><block wx:if="{{user.isAssistant==0}}"><block><van-cell-group vue-id="5" title="个人信息" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><van-cell vue-id="{{('6')+','+('5')}}" title="姓名" value="{{user.trueName}}" data-com-type="wx" bind:__l="__l"></van-cell><van-cell vue-id="{{('7')+','+('5')}}" title="学号" value="{{user.username}}" data-com-type="wx" bind:__l="__l"></van-cell><van-cell vue-id="{{('8')+','+('5')}}" title="专业" value="{{user.studentMajorName}}" data-com-type="wx" bind:__l="__l"></van-cell><van-cell vue-id="{{('9')+','+('5')}}" title="学院" value="{{user.college}}" data-com-type="wx" bind:__l="__l"></van-cell><block wx:if="{{user.majorNumber}}"><block><van-cell vue-id="{{('10')+','+('5')}}" title="专业班级号:" value="{{user.majorNumber}}" data-com-type="wx" bind:__l="__l"></van-cell></block></block></van-cell-group></block></block><block wx:else><block><van-cell-group vue-id="11" title="个人信息" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><van-cell vue-id="{{('12')+','+('11')}}" title="专业号" value="{{user.majorNumber}}" data-com-type="wx" bind:__l="__l"></van-cell></van-cell-group></block></block></view><view class="main-logout"><van-button vue-id="13" color="#FF0000" round="true" data-event-opts="{{[['^click',[['logout']]]]}}" data-com-type="wx" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">退出登陆</van-button></view></view><view class="footer"></view></view></scroll-view>