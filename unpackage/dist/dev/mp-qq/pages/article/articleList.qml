<view class="articleList"><view class="content"><block qq:for="{{articleList}}" qq:for-item="item" qq:for-index="index" qq:key="index"><block><view class="article" id="{{item.id}}"><view data-event-opts="{{[['tap',[['toArticleContent',['$0'],[[['articleList','',index,'id']]]]]]]}}" class="article-header" bindtap="__e"><view class="article-header-author"><image src="{{item.articleUser.qqAvatarUrl!=''?item.articleUser.qqAvatarUrl:item.articleUser.wxAvatarUrl}}"></image><text>{{"作者："+item.author}}</text></view><view class="article-header-time"><text>{{"发表时间："+item.createTime}}</text></view></view><view data-event-opts="{{[['tap',[['toArticleContent',['$0'],[[['articleList','',index,'id']]]]]]]}}" class="article-title" bindtap="__e"><text>{{item.title}}</text></view><view class="article-layout"><image src="{{item.layout}}" mode="aspectFit" data-event-opts="{{[['tap',[['showImage',['$0'],[[['articleList','',index,'layout']]]]]]]}}" bindtap="__e"></image></view><view data-event-opts="{{[['tap',[['toArticleContent',['$0'],[[['articleList','',index,'id']]]]]]]}}" class="article-abstract" bindtap="__e"><text>{{"摘要："+item.articleAbstract}}</text></view><view data-event-opts="{{[['tap',[['toArticleContent',['$0'],[[['articleList','',index,'id']]]]]]]}}" class="article-label" bindtap="__e"><uni-tag vue-id="{{'5886e6fc-1-'+index}}" type="error" circle="true" size="small" text="{{item.articleKind1}}" bind:__l="__l"></uni-tag><uni-tag vue-id="{{'5886e6fc-2-'+index}}" type="success" circle="true" size="small" text="{{item.articleKind2}}" bind:__l="__l"></uni-tag></view></view></block></block></view></view>